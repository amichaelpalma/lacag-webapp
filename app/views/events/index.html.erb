<div class="text-center"> 
  <h1>Events Page</h1> 
  <p><%= link_to 'New Event', new_event_path, class: 'btn btn-outline-primary' %>
  </p>
</div>
<br>


<% @events.each do |event| %>

<div class="border px-2 pb-2 mb-2">
<h1><%= event.title %></h1>
<h4><%= event.subtitle %></h4>
<p class="btn btn-sm btn-success"><%= event.tag %></p>
<hr style="border: 1px solid blue;">
<p><%= event.details %></p>

<div>
  <%= link_to 'Edit', edit_event_path(event), class: 'btn btn-outline-primary' %>
  <%= link_to 'Show', event_path(event), class: 'btn btn-outline-primary' %>
</div>
</div>

<% end %>





