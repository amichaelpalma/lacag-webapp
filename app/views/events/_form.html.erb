

<%= form_for @event do |f| %>

<p>
  <%= f.label :title %> <br>
  <%= f.text_field :title %>
</p>

<p>
  <%= f.label :subtitle %> <br>
  <%= f.text_field :subtitle %>
</p>

<p>
  <%= f.label :tag %> <br>
  <%= f.text_field :tag %>
</p>

<p>
  <%= f.label :details %> <br>
  <%= f.text_area :details %>
</p>

<p>
  <%= f.submit %>
</p>


<% end %>